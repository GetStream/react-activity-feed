{
  "name": "react-activity-feed",
  "version": "0.10.8",
  "description": "React components to create activity and notification feeds",
  "author": "GetStream",
  "license": "BSD-3-Clause",
  "repository": "GetStream/react-activity-feed",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "jsnext:main": "dist/index.es.js",
  "jsdelivr": "dist/browser.full-bundle.js",
  "files": [
    "src",
    "dist"
  ],
  "engines": {
    "node": "10 || 12 || >=14"
  },
  "scripts": {
    "test": "jest",
    "eslint": "eslint '**/*.{js,md}' --max-warnings 0",
    "prettier": "prettier --list-different '**/*.{js,ts,md,json}' .eslintrc.json .prettierrc .babelrc",
    "prettier-fix": "prettier --write '**/*.{js,ts,md,json}' .eslintrc.json .prettierrc .babelrc",
    "validate-translations": "node scripts/validate-translations.js",
    "lint": "yarn run prettier && yarn run eslint && stylelint '**/*.{css,scss}'",
    "lint-fix": "yarn run prettier-fix && yarn run eslint --fix && stylelint --fix '**/*.{css,scss}'",
    "build": "rollup -c",
    "start": "rollup -c -w",
    "docs": "styleguidist build",
    "docs-server": "styleguidist server",
    "size": "size-limit",
    "analyze": "size-limit --why",
    "prepare": "yarn run build",
    "preversion": "yarn install && yarn lint && yarn test",
    "version": "yarn docs && git add -A docs",
    "postversion": "git push && git push --tags && npm publish"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn run lint"
    }
  },
  "stylelint": {
    "extends": "stylelint-config-standard",
    "rules": {
      "no-descending-specificity": null,
      "no-duplicate-selectors": null,
      "block-no-empty": null,
      "declaration-block-no-duplicate-properties": null
    }
  },
  "peerDependencies": {
    "react": "^17.0.0 || ^16.8.0",
    "react-dom": "^17.0.0 || ^16.8.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.16",
    "@babel/core": "^7.12.16",
    "@babel/node": "^7.12.16",
    "@babel/plugin-external-helpers": "^7.12.13",
    "@babel/plugin-proposal-class-properties": "^7.12.13",
    "@babel/plugin-transform-runtime": "^7.12.15",
    "@babel/preset-env": "^7.12.16",
    "@babel/preset-react": "^7.12.13",
    "@babel/register": "^7.12.13",
    "@babel/runtime-corejs2": "^7.12.13",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^17.1.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^11.2.0",
    "@rollup/plugin-typescript": "^8.2.0",
    "@rollup/plugin-url": "^6.0.0",
    "@size-limit/preset-big-lib": "^4.9.2",
    "@types/react": "^17.0.2",
    "@types/react-dom": "^17.0.1",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.2.2",
    "babel-plugin-i18next-extract": "^0.8.2",
    "eslint": "^7.19.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-config-react-app": "^6.0.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-babel": "^5.3.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.1.3",
    "eslint-plugin-jest-dom": "^3.6.5",
    "eslint-plugin-markdown": "^1.0.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-promise": "^4.3.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-sonarjs": "^0.5.0",
    "faker": "^4.1.0",
    "husky": "^4.3.0",
    "jest": "^26.6.3",
    "postcss": "^8.2.6",
    "postcss-color-mod-function": "^3.0.3",
    "postcss-cssnext": "^3.1.0",
    "postcss-easy-import": "^3.0.0",
    "postcss-import": "^14.0.0",
    "postcss-loader": "^4.2.0",
    "postcss-nested": "^5.0.3",
    "postcss-safe-parser": "^5.0.2",
    "postcss-simple-vars": "^6.0.3",
    "prettier": "^2.2.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-styleguidist": "^8.0.6",
    "react-test-renderer": "^17.0.1",
    "rollup": "^2.39.0",
    "rollup-plugin-copy": "^3.3.0",
    "rollup-plugin-node-globals": "^1.4.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-postcss": "^4.0.0",
    "size-limit": "^4.9.2",
    "stylelint": "^13.9.0",
    "stylelint-config-standard": "^20.0.0",
    "ts-jest": "^26.5.1",
    "tslib": "^2.1.0",
    "typescript": "^4.1.5",
    "url-loader": "^1.1.2",
    "webpack": "^4.12.2"
  },
  "dependencies": {
    "@babel/runtime": "^7.12.13",
    "@fortawesome/fontawesome-svg-core": "^1.2.34",
    "@fortawesome/free-regular-svg-icons": "^5.15.2",
    "@fortawesome/react-fontawesome": "^0.1.14",
    "@webscopeio/react-textarea-autocomplete": "^4.7.3",
    "anchorme": "^1.1.2",
    "dayjs": "^1.10.4",
    "emoji-mart": "^3.0.0",
    "getstream": "^7.2.1",
    "i18next": "^19.8.7",
    "immutable": "^4.0.0-rc.12",
    "lodash": "^4.17.20",
    "react-file-utils": "^0.4.0",
    "react-images": "^0.5.19",
    "stream-analytics": "^3.4.2",
    "twitter-text": "^3.1.0",
    "url-parse": "^1.4.7"
  },
  "keywords": [
    "feed",
    "newsfeed",
    "activity stream",
    "react",
    "notification feed",
    "stream",
    "getstream",
    "getstream.io"
  ],
  "size-limit": [
    {
      "path": "dist/index.es.js",
      "limit": "320 KB",
      "import": "{ createComponent }"
    },
    {
      "path": "dist/index.css",
      "limit": "10 KB",
      "webpack": false
    }
  ]
}
